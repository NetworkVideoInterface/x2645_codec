#pragma code_page(65001)

#define BUILD_TIME @BUILD_TIME_YEAR@,@BUILD_TIME_MONTH@,@BUILD_TIME_DAY@,@BUILD_TIME_TIME@
#define BUILD_TIME_STR "@BUILD_TIME_YEAR@.@BUILD_TIME_MONTH@.@BUILD_TIME_DAY@.@BUILD_TIME_TIME@"
#define PROJECT_VERSION_NUMBER @LIB_VERSION_MAJOR@,@LIB_VERSION_MINOR@,@LIB_VERSION_PATCH@,0
#define PROJECT_VERSION_NUMBER_STR "@LIB_VERSION_MAJOR@.@LIB_VERSION_MINOR@.@LIB_VERSION_PATCH@"
#define PUBLISHER_NAME "@LIB_PUBLISHER@"
#define COPYRIGHT_NAME "@LIB_COPYRIGHT@"

#include <windows.h>

1 VERSIONINFO
  FILETYPE VFT_DLL
  FILEOS VOS_NT_WINDOWS32
  PRODUCTVERSION PROJECT_VERSION_NUMBER
  FILEVERSION BUILD_TIME
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "000904e4"
    BEGIN
      VALUE "CompanyName", PUBLISHER_NAME
      VALUE "ProductName", "X2645 Plugin"
      VALUE "ProductVersion", PROJECT_VERSION_NUMBER_STR
      VALUE "FileVersion", BUILD_TIME_STR
      VALUE "FileDescription", "X2645 plugin runtime library for NVI®"
      VALUE "InternalName", "X2645Plugin"
      VALUE "OriginalFilename", "X2645Plugin.dll"
      VALUE "LegalCopyright", "©@BUILD_TIME_YEAR@ " COPYRIGHT_NAME
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x9, 1252
  END
END
